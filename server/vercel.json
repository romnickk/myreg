{
    "version":2,
    "builds":[
        {
            "src":"*.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/api(.*)",
            "dest":"/index/$1"
        },
        {
            "src":"/api/display",
            "methods":[
                "POST",
                "GET"
            ],
            "dest":"/api/userRoute.js"
        },
        {
            "src":"/api/register",
            "dest":"/api/userRoute.js"
        }
    ]
}